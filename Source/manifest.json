{
	"manifest_version": 2,
	"name": "Chrome Web Tools",
	"version": "2.7",
	"description": "Additional functionality for web developers",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "icon-256.png",
		"32": "icon-256.png",
		"48": "icon-256.png",
		"64": "icon-256.png",
		"128": "icon-256.png",
		"256": "icon-256.png"
	}
}

